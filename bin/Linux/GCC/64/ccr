#!/bin/bash
# GCC C++ Compile Release
g++ -pipe -std=c++11 -pedantic -Wall -Wextra -Winline -Wno-unused-parameter -Wno-unused-but-set-parameter -Wno-unused-variable -Wno-unused-but-set-variable -Wno-unused-label -Wno-unused-function -Wno-unknown-pragmas -ffor-scope -m64 -march=native -DNDEBUG -Ofast -fno-stack-protector -fcilkplus -Wno-maybe-uninitialized -s -c $@
